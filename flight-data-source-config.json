{
  "name": "MySQL_Source",
  "config":    {
      "connector.class": "org.apache.kafka.connect.jdbc.JdbcSourceConnector",
      "tasks.max": 1,
      "connection.url": "jdbc:mysql://mysql-db:3306/flights?user=root&password=confluent",
      "table.whitelist": "airlines,flights,airports,routes,users,trips,planes,countries,facebook,locales",
      "mode": "timestamp",
      "timestamp.column.name": "modified",
      "poll.interval.ms": 500,
      "topic.prefix": "mysql-"
    }
}
